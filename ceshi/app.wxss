/**app.wxss**/
.page{
  font-family: microsoft yehei;
}
/* 导航栏 */
.navbars{
  position: relative;
  width: 100%;
  height: 76rpx;
}
.navbars .title{
  position: absolute;
  margin-left:304rpx; 
  font-size:36rpx; 
  color:rgba(3,3,3,1);
  line-height: 50rpx;
  width: 144rpx;
  height: 50rpx;
  text-align: center; 
}
.navbars .backs image{
  width: 100%;
  height: 100%;
}
.navbars .backs {
  position: absolute;
  width: 24rpx;
  height: 40rpx;
  margin-top:4rpx;
  margin-left:28rpx;
}
::-webkit-scrollbar {
     width: 0;
     height: 0;
     color: transparent;
}